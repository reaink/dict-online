{
  "name": "dict-online-server",
  "version": "1.0.0",
  "license": "MIT",
  "description": "",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --no-notify --respawn --transpile-only src/server",
    "start": "NODE_ENV=production node -r ts-node/register -r tsconfig-paths/register dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist && tscp -p tsconfig.json",
    "generate": "NODE_ENV=development pal g && npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "NODE_ENV=development prisma generate",
    "generate:nexus": "NODE_ENV=development ts-node -r tsconfig-paths/register --transpile-only src/nexusSchema",
    "init:role": "ts-node -r tsconfig-paths/register --transpile-only ./src/tools/initRole.ts",
    "init:permission": "ts-node -r tsconfig-paths/register --transpile-only ./src/tools/initPermission.ts",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "@paljs/nexus": "4.1.0",
    "@paljs/plugins": "^4.1.0",
    "@prisma/client": "3.14.0",
    "ali-oss": "^6.17.1",
    "apollo-server": "^3.9.0",
    "apollo-server-core": "^3.9.0",
    "apollo-server-koa": "^3.9.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "casbin": "5.15.2",
    "casbin-prisma-adapter": "^1.1.3",
    "co": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.1",
    "graphql": "16.5.0",
    "graphql-authz": "^0.0.1",
    "graphql-middleware": "^6.1.31",
    "graphql-shield": "^7.5.0",
    "graphql-upload": "13.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-jwt": "^4.0.3",
    "lodash": "^4.17.21",
    "nexus": "1.3.0",
    "nexus-validate": "^1.2.0",
    "subscriptions-transport-ws": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@faker-js/faker": "^7.3.0",
    "@paljs/cli": "4.1.0",
    "@types/ali-oss": "^6.16.4",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/graphql-upload": "^8.0.11",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "graphql-scalars": "^1.17.0",
    "miniprogram-api-typings": "^3.5.0",
    "prettier": "2.7.1",
    "prisma": "3.14.0",
    "ts-node": "10.8.2",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tscpaths": "^0.0.9",
    "typescript": "4.7.4",
    "typescript-cp": "^0.1.5"
  },
